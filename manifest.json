{
  "manifest_version": 3,
  "name": "AD Skipper",
  "version": "1.0.0",
  "description": "A basic example extension that skips ads ",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["Skipper.js"]
    }
  ],
  "background": {
    "service_worker": "worker.js"
  }
}
